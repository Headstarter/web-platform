[(1, '3d2f8616-f3be-45c4-8a0c-fda9702e1ff6', 'Codix Bulgaria', '<blockquote class="blockquote"><p class="codix-container-title"> Software solution for Commercial Finance, Supply chain Finance, Consumer Finance & Debt collection</p></blockquote>\n          \n          CODIX is the software company, which has developed iMX – a \nunique software solution, providing businesses with an event-driven \nsystem for the management of their activities, such as Factoring, \nCommercial Finance, Supply Chain Finance (SCF), Trade and Corporate \nFinance, Debt Collection and Legal, Accounts Receivable, Consumer \nFinance, Leasing, Credit Insurance, etc. Given the achievements and \ncapabilities of iMX, it is one of the best commercial finance softwares \non the market.\n            <br><br>\n          iMX is an innovative software solution, which enables our \nclients to manage all business processes within a single technical \nstructure, which can be easily customised to meet their specific needs. \nTo see how iMX software handles commercial finance system and it\'s full \ncapabilities, check out our Line of Business pages.\n            ', '/images/company/3d2f8616-f3be-45c4-8a0c-fda9702e1ff6.png', 'https://www.codix.eu/en', ''), (2, '788a74dc-f406-45fd-ad91-7224dfabd12e', 'Headstarter', '', '/images/company/788a74dc-f406-45fd-ad91-7224dfabd12e.png', 'https://headstarter.eu', None), (3, '9cbd550e-c3ea-4e60-8eef-350345cd6d37', 'Списание 8', '', '/images/company/9cbd550e-c3ea-4e60-8eef-350345cd6d37.png', '', None), (4, '889a59b4-30f5-44a8-81ad-8ec191b9ad86', 'BIODIT', '<span id="docs-internal-guid-f4f1f5cb-7fff-4f5d-cc90-4646d9b4ac82" style="font-family: -webkit-standard;"><span style="font-size: 11pt; font-family: Georgia; background-color: transparent; font-variant-ligatures: normal; font-variant-east-asian: normal; font-variant-position: normal; vertical-align: baseline; white-space: pre-wrap;">BIODIT AD is a leading high-tech company specializing in the development and manufacture of security, access control, hospitality and time attendance systems, based on biometric identification. Our innovative products combine state-of-the-art identification and authentication technologies with fingerprint recognition.</span></span>', '/images/company/889a59b4-30f5-44a8-81ad-8ec191b9ad86.png', 'www.biodit.com', None), (5, '05343420-8480-4551-8a3b-a5390739aca5', 'HRS Bulgaria', '', '/images/company/05343420-8480-4551-8a3b-a5390739aca5.png', '', None)]
